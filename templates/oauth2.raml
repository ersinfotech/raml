#%RAML 0.8
title: title
baseUri: !include _config/base_url
securedBy: [oauth_2_0]

securitySchemes:
  - oauth_2_0:
      type: OAuth 2.0
      describedBy:
        headers:
          Authorization:
        queryParameters:
          access_token:
      settings:
        clientId: !include _config/client_id
        authorizationUri: !include _config/authorization_url
        accessTokenUri: !include _config/access_token_url
        authorizationGrants: [token]
